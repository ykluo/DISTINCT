<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>distinct</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>NameQueryServlet</servlet-name>
    <servlet-class>com.distinct.servlet.NameQueryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NameQueryServlet</servlet-name>
    <url-pattern>/author</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ResultQueryServlet</servlet-name>
    <servlet-class>com.distinct.servlet.ResultQueryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ResultQueryServlet</servlet-name>
    <url-pattern>/paper</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.distinct.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RequestServlet</servlet-name>
    <servlet-class>com.distinct.servlet.RequestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RequestServlet</servlet-name>
    <url-pattern>/message</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>DecisionServlet</servlet-name>
  	<servlet-class>com.distinct.servlet.DecisionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>DecisionServlet</servlet-name>
  	<url-pattern>/confirm</url-pattern>
  </servlet-mapping>
</web-app>